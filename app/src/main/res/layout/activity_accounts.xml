<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.jsr268client.Accounts"
    android:background="@drawable/bg">

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:text="Start Date"

        android:id="@+id/startbutton"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:onClick="showDialogOnButtonClick1"
        android:background="#23297A"
        android:textColor="#ffffff" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:text="End Date"
        android:id="@+id/endbutton"
        android:layout_below="@+id/startbutton"
        android:layout_alignRight="@+id/startbutton"
        android:onClick="showDialogOnButtonClick2"
        android:background="#23297A"
        android:textColor="#ffffff"
        android:layout_marginTop="10dp" />

    <Button
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:text="Generate"
        android:id="@+id/gene"
        android:onClick="gen"
        android:background="@android:color/holo_green_dark"
        android:layout_below="@+id/endbutton"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Large Text"
        android:id="@+id/strd"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/startbutton"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="15dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Large Text"
        android:id="@+id/endd"
        android:layout_below="@+id/startbutton"
        android:layout_alignLeft="@+id/strd"
        android:layout_marginTop="20dp" />

    <ListView android:id="@+id/listView4" android:layout_width="fill_parent"
        android:layout_marginTop="90dp"
        android:layout_height="900dp"
        android:layout_below="@+id/gene"
        android:layout_alignParentRight="true" />

    <Button
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:text="Total"
        android:id="@+id/button10"
        android:layout_above="@+id/gene"
        android:layout_toLeftOf="@+id/textView23"
        android:layout_marginRight="0dp"
        android:background="@drawable/newbutton"
        android:layout_marginBottom="25dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Item"
        android:id="@+id/textView6"
        android:layout_below="@+id/gene"
        android:layout_alignParentLeft="true"
        android:layout_marginTop="20dp"
        android:layout_marginLeft="20dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Cost/Item"
        android:id="@+id/textView20"
        android:layout_alignTop="@+id/textView6"
        android:layout_toRightOf="@+id/textView6"
        android:layout_marginLeft="100dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Quantity"
        android:id="@+id/textView22"
        android:layout_alignTop="@+id/textView20"
        android:layout_toRightOf="@+id/textView20"
        android:layout_marginLeft="100dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Total"
        android:id="@+id/textView23"
        android:layout_alignTop="@+id/textView22"
        android:layout_alignParentRight="true" />

</RelativeLayout>
